{
    "name": "techlify/laravel-saas-users",
    "description": "User Management & Role Based Access Control for Laravel",
    "type": "package",
    "require": {
        "php": ">=7.1.3",
        "doctrine/dbal": ">=2.6",
        "laravel/framework": ">=5.6",
        "laravel/passport": ">=6.0",
        "laravel/tinker": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Techlify Inc.",
            "email": "info@techlify.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "TechlifyInc\\LaravelSaasUser\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TechlifyInc\\LaravelSaasUser\\LaravelSaasUserServiceProvider"
            ],
            "aliases": {
                "Debugbar": "TechlifyInc\\LaravelSaasUser\\LaravelSaasUserFacade"
            }
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    }
}
